{
  "version": 2,
  "functions": {
    "api/*.php": { "runtime": "vercel-php@0.7.4" }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/api/index.php"
    },
    {
      "src": "/login",
      "dest": "/api/index.php"
    },
    {
      "src": "/logout",
      "dest": "/api/index.php"
    },
    {
      "src": "/flats/(.*)",
      "dest": "/api/index.php"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.php"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}
