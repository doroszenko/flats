    "description": "Aplikacja do zarządzania opłatami za media w mieszkaniach",
    "type": "project",
    "require": {
        "php": "^8.1",
        "slim/slim": "^4.12",
        "slim/psr7": "^1.6",
        "slim/twig-view": "^3.3",
        "twig/twig": "^3.0",
        "monolog/monolog": "^3.0",
        "php-di/php-di": "^7.0",
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
